{
    "name": "adelplace/onesignal-bundle",
    "description": "Symfony integration of the norkunas/onesignal-php-api library",
    "type": "library",
    "require": {
        "symfony/symfony": "^2.6",
        "guzzlehttp/guzzle": "^6.0",
        "norkunas/onesignal-php-api": "dev-master",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "adelplace",
            "email": "alexandre.delplacemille@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src",
            "vendor/norkunas/onesignal-php-api/src"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "vendor/norkunas/onesignal-php-api/src"
        ]
    }
}
